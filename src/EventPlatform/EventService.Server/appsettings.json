{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Keycloak": {
    "realm": "EventPlatform",
    "auth-server-url": "http://keycloak:8080/",
    "ssl-required": "none",
    "resource": "eventplatform-client",
    "public-client": true,
    "confidential-port": 0
  }
}
